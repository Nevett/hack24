@inherits WebViewPage<Hack24.Core.Entities.Question>

@Model.Text

<div class="row">
    <ul data-questionId="@Model.Id">
	    @foreach (var answer in Model.Answers)
    {
	<li class="answer" data-answerId="@answer.Id">
		    <div>
			    <span>@answer.Text</span>
			    <img src="@answer.ImageUrl" />
		    </div>
	    </li>
    }
    </ul>
</div>